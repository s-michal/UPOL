        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:primes"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500">
    <Grid>
        <Label Content="Zadej číslo:" HorizontalAlignment="Left" Margin="72,118,0,0" VerticalAlignment="Top"/>
        <Label Content="Prvočísla: " HorizontalAlignment="Left" Margin="72,162,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="num" HorizontalAlignment="Left" Height="26" Margin="146,118,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="140"/>
        <TextBox x:Name="output" HorizontalAlignment="Left" Height="136" Margin="146,162,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="246"/>
        <Button x:Name="vypisButton" Content="vypiš" HorizontalAlignment="Left" Height="27" Margin="292,117,0,0" VerticalAlignment="Top" Width="100" Click="vypisButton_Click"/>
        <Label x:Name="error" Content="" HorizontalAlignment="Left" Height="26" Margin="147,70,0,0" VerticalAlignment="Top" Width="245"/>

    </Grid>
</Window>
